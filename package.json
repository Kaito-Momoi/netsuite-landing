{
  "name": "netsuite-landing",
  "version": "0.1.0",
  "homepage": "https://Kaito-Momoi.github.io/netsuite-landing",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cd evangsol-site && npm install && npm run build",
    "build:original": "react-scripts build",
    "postbuild:original": "react-snap",
    "build:static": "react-scripts build && react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "reactSnap": {
    "inlineCss": true,
    "source": "build",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-gpu"
    ],
    "puppeteerExecutablePath": "/usr/bin/chromium",
    "puppeteerIgnoreHTTPSErrors": true,
    "skipThirdPartyRequests": true,
    "publicPath": "/netsuite-landing/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "playwright": "^1.56.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer-core": "^24.24.1",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.17"
  },
  "packageManager": "npm@11.4.2"
}
